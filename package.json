  "scripts": {
    "preparestart": "node ./node_modules/gulp/bin/gulp.js prepare && node scripts/start.js",
    "start:local": "node ./node_modules/gulp/bin/gulp.js prepare --env local && node scripts/start.js",
    "start:talentwand": "VQ_API_URL=https://talentwand.vqmarketplace.com/api npm run preparestart",
    "start:ani": "VQ_API_URL=https://ani.vqmarketplace.com/api npm run preparestart",
    "start:b2btemplate": "VQ_API_URL=https://b2btemplate.vqmarketplace.com/api npm run preparestart",
    "start:click4work": "VQ_API_URL=https://click4work.vq-labs.com/api npm run preparestart",
    "start:rentkitchen": "VQ_API_URL=https://rental-kitchen.vqmarketplace.com/api npm run preparestart",
    "start:bitcoinmeetup": "VQ_API_URL=https://bitcoinmeetup.vqmarketplace.com/api npm run preparestart",
    "start:yogaat": "VQ_API_URL=https://yogaat.vqmarketplace.com/api npm run preparestart",
    "build": "node ./node_modules/gulp/bin/gulp.js prepare --env production && node scripts/build.js && npm run build-rename",
    "build-rename": "npm run build-rename-js && npm run build-rename-css",
    "build-rename-js": "node ./node_modules/renamer/bin/cli --regex --find 'main\\.[^\\.]+\\.js' --replace 'main.js' build/static/js/*.js",
    "build-rename-css": "node ./node_modules/renamer/bin/cli --regex --find 'main\\.[^\\.]+\\.css' --replace 'main.css' build/static/css/*.css",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
